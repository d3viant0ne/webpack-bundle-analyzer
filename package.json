{
  "name": "webpack-bundle-analyzer",
  "version": "2.9.0",
  "description": "Webpack plugin and CLI utility that represents bundle content as convenient interactive zoomable treemap",
  "author": "Yury Grunin <grunin.ya@ya.ru>",
  "license": "MIT",
  "homepage": "https://github.com/th0r/webpack-bundle-analyzer",
  "changelog": "https://github.com/th0r/webpack-bundle-analyzer/blob/master/CHANGELOG.md",
  "bugs": {
    "url": "https://github.com/th0r/webpack-bundle-analyzer/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/th0r/webpack-bundle-analyzer.git"
  },
  "main": "dist/cjs.js",
  "bin": "lib/bin/analyzer.js",
  "engines": {
    "node": ">= 4.3 < 5.0.0 || >= 5.10"
  },
  "scripts": {
    "start": "npm run build -- -w",
    "build": "cross-env NODE_ENV=production babel src -d dist --ignore 'src/**/*.test.js'",
    "npm-publish": "npm run lint && npm run build && npm test && npm publish",
    "lint": "eslint --cache src test",
    "test": "jest",
    "test-dev": "mocha --watch --compilers js:babel-core/register",
    "webpack-defaults": "webpack-defaults",
    "appveyor:test": "npm run test",
    "clean": "del-cli dist",
    "lint-staged": "lint-staged",
    "prebuild": "npm run clean",
    "prepublish": "npm run build",
    "release": "standard-version",
    "security": "nsp check",
    "test:watch": "jest --watch",
    "test:coverage": "jest --collectCoverageFrom='src/**/*.js' --coverage",
    "travis:lint": "npm run lint && npm run security",
    "travis:test": "npm run test -- --runInBand",
    "travis:coverage": "npm run test:coverage -- --runInBand"
  },
  "files": [
    "public",
    "lib",
    "src",
    "views",
    "dist"
  ],
  "dependencies": {
    "acorn": "^5.1.1",
    "chalk": "^1.1.3",
    "commander": "^2.9.0",
    "ejs": "^2.5.6",
    "express": "^4.15.2",
    "filesize": "^3.5.9",
    "gzip-size": "^3.0.0",
    "lodash": "^4.17.4",
    "mkdirp": "^0.5.1",
    "opener": "^1.4.3",
    "ws": "^2.3.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "6.24.1",
    "babel-eslint": "7.2.3",
    "babel-jest": "^21.2.0",
    "babel-loader": "7.0.0",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-object-rest-spread": "6.23.0",
    "babel-plugin-transform-react-jsx": "6.24.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "1.6.0",
    "chai": "3.5.0",
    "chai-subset": "1.5.0",
    "classnames": "2.2.5",
    "cross-env": "^5.0.5",
    "css-loader": "0.28.1",
    "del": "2.2.2",
    "del-cli": "^1.1.0",
    "eslint": "3.19.0",
    "eslint-config-webpack": "^1.2.5",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-react": "7.0.0",
    "exports-loader": "0.6.4",
    "jest": "^21.2.1",
    "lint-staged": "^4.2.3",
    "mocha": "3.3.0",
    "nightmare": "2.10.0",
    "nsp": "^2.8.1",
    "pre-commit": "^1.2.2",
    "preact": "8.1.0",
    "sinon": "2.2.0",
    "standard-version": "^4.2.0",
    "stream-combiner2": "1.1.1",
    "style-loader": "0.17.0",
    "webpack": "2.5.1",
    "webpack-defaults": "^1.6.0",
    "webpack-dev-server": "2.4.5"
  },
  "keywords": [
    "webpack",
    "bundle",
    "analyzer",
    "modules",
    "size",
    "interactive",
    "chart",
    "treemap",
    "zoomable",
    "zoom"
  ],
  "peerDependencies": {
    "webpack": "^2.0.0 || ^3.0.0"
  },
  "pre-commit": "lint-staged",
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  }
}
